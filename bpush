#!/bin/bash

. $HOME/backup.cfg

myname=`basename $0`
mlog=$log-$myname.log

echo `date` $myname - S3 file pushing $datestr started >$mlog
echo Dir: $bdir Bucket: $s3bucket >>$mlog

s3cmd --no-progress --skip-existing --delete-removed sync $bdir s3://$s3bucket 1>>$mlog 2>>$mlog

echo `date` $myname - S3 file pushing $datestr started >>$mlog

send_report $myname' at '$whost $mlog;
rm -f $mlog

